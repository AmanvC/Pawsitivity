{
  "expo": {
    "name": "Pawsitivity",
    "slug": "pawsitivity",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dog.jpeg",
    "TODO - AMAN": "Change the main icon here",
    "scheme": "pawsitivity",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dog.jpeg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/happy-dog.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff",
          "enableFullScreenImage_legacy": true
        }
      ],
      "expo-secure-store"
    ],
    "expo-font": {
      "fonts": [
        "./assets/fonts/Rubik-Bold.ttf",
        "./assets/fonts/Rubik-ExtraBold.ttf",
        "./assets/fonts/Rubik-Light.ttf",
        "./assets/fonts/Rubik-Medium.ttf",
        "./assets/fonts/Rubik-Regular.ttf",
        "./assets/fonts/Rubik-SemiBold.ttf"
      ]
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
